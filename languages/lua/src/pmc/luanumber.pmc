        FLOATVAL a = SELF.get_number();
        FLOATVAL b = VTABLE_get_number(INTERP, value);
        dest       = pmc_new(INTERP, dynpmc_LuaNumber);

        VTABLE_set_number_native(INTERP, dest, a - floor(a/b)*b);

        return dest;
    }

