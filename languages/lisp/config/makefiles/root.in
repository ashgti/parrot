# $Id$

# Setup some commands
RM_F = @rm_f@
PERL = @perl@
PARROT = ../../parrot@exe@

all: build

test: build
	$(PERL) -Ilib -I../../lib t/harness

build: lisp.pir
	$(PARROT) -o lisp.pbc lisp.pir

clean: testclean
	$(RM_F) core "*.pbc" "*~"

testclean:
	$(RM_F) t/*.out t/*.l

realclean: clean
	$(RM_F) Makefile
