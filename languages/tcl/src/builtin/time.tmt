[time command:script ?count:int=1?]

  $I0 = $count
  .local float t
  t = time
time_loop:
  if $I0 == 0 goto time_done
  
  $command
  
  dec $I0
  goto time_loop
  
time_done:
  $N0 = time
  t -= $N0
  t *= 1000000
  
  $N0 = $count
  t /= $N0
  
  $S0 = t
  $S0 .= ' microseconds per iteration'
  
  $R = new .TclString
  $R = $S0
