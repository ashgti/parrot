# [expr %]

.sub __operand_mod
  .param pmc argv

  # requires 2 args, numeric only

  .local pmc result 
  result = new PerlArray

  # XXX assuming numeric

  # XXX also assuming 2 args.
  pop $P0, argv
  pop $P1, argv

  $I0 = $P0[1]
  $I1 = $P1[1]

  mod $I2, $I0, $I1

  result[0] = INTEGER
  result[1] = $I2

  push argv, result

  .pcc_begin_return
  .return argv
  .return INTEGER
  .pcc_end_return   
.end
