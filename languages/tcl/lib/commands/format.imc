###
# [format]

#
# format string arg arg arg

.namespace [ "Tcl" ]

.sub _format
  .local pmc argv 
  argv = foldup
  
  .local string format

  # pull off the format string.
  shift format, argv

  sprintf $S0, format, argv 

  $P0 = new String
  $P0 = $S0

done:
  .pcc_begin_return
    .return TCL_OK
    .return $P0
  .pcc_end_return
.end
