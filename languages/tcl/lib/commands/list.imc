##
# [list]

.namespace [ "Tcl" ]

.sub _list

  .local pmc argv 
  argv = foldup

  # convert the Array ireturned by foldup into a TclList.

  .local int argc
  argc = argv
 
  .local int TclList
  TclList = find_type "TclList"
  .local pmc retval
  retval = new TclList
 
  .local int cnt
  cnt = 0
LOOP:
  if cnt >= argc goto DONE
  $P0 = argv[cnt]
  retval[cnt] = $P0
  inc cnt
  goto LOOP
DONE: 

  .return(0,retval)
.end
