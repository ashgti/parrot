ROOT: result(.) = {
    .local pmc result
    result = new 'POST::Sub'
    result.'clone'(node)

    .local pmc iter
    iter = node.'child_iter'()
  iter_loop:
    unless iter, iter_end         # while (entries) ...
      shift $P2, iter
      $P3 = tree.get('result', $P2)
      result.'add_child'($P3)
      goto iter_loop
  iter_end:

    .return (result)
}

