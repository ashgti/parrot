    PMC* add(PMC* value, PMC* dest) {
MMD_WmlsInteger: {
            INTVAL result = SELF.get_integer()
                          + VTABLE_get_integer(INTERP, value);
            dest = pmc_new(INTERP, dynpmc_WmlsInteger);
            VTABLE_set_integer_native(INTERP, dest, result);
            return dest;
