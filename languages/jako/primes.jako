const int n = 100;

var   int i =   2;

print("Algorithm P (Naiive primality test)\n");
print("  Printing primes up to $n...\n");

NUMBER: while (i <= n) {
  var int m;
  var int j = 2;

  m = i / 2;

  FACTOR: while (j <= m) {
    var int x;

    x = i % j;

    next NUMBER if (x == 0);

    j++;
  }

  print("$i  ");
} continue {
  i++;
}

print("\n");

