const int n = 24;

var   int a =  1;
var   int b =  1;
var   int f =  1; 
var   int i =  3;

print("Algorithm F2 (Fibonacci's function)\n");
print("  Calculating fib($n) = ...\n");

while (i <= n) {
  f = a + b;
  a = b;
  b = f;
  i++;
}

print("  ... = $f\n");
