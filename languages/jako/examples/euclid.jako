sub print {op} (str a);

var int m, n, r;

m = 96;
n = 64;

print("Algorithm E (Euclid's algorithm)\n");
print("  Calculating gcd($m, $n) = ...\n");

r = m % n;
while (r != 0) {
  m = n;
  n = r;
  r = m % n;
} 

print("  ... = $n\n");
