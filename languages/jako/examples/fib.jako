use sys;

const int n = 24;

var   int a =  1;
var   int b =  1;
var   int f =  1; 
var   int i =  3;

sys::print("Algorithm F2 (Fibonacci's function)\n");
sys::print("  Calculating fib($n) = ...\n");

while (i <= n) {
  f = a + b;
  a = b;
  b = f;
  i++;
}

sys::print("  ... = $f\n");

