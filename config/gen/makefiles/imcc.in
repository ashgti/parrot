RM_F = ${rm_f}
PERL = ${perl}

all :
	@echo "Don't make imcc, make parrot"

testclean:
	$(RM_F)  \
	        t/*/*.out t/*/*.imc \
		t/*/*.pbc t/*/*.o t/*/*.stabs.s t/*/*.pasm

clean:	testclean
	$(RM_F) core \
	        *${o} \
	        imcparser.output

realclean: clean

test:
	$(PERL) t/harness

testj:
	$(PERL) t/harness -j

testP:
	$(PERL) t/harness -P

testb:
	$(PERL) t/harness -b

