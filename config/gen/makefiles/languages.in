MAKE = ${make}

all : imcc jako scheme

imcc: imcc.dummy

imcc.dummy:
	cd imcc && $(MAKE) && cd ..

jako: jako.dummy imcc.dummy

jako.dummy:
	cd jako && $(MAKE) && cd ..

scheme: scheme.dummy

scheme.dummy:
	cd scheme && $(MAKE) && cd ..

clean:
	cd imcc && $(MAKE) clean && cd ..
	cd jako && $(MAKE) clean && cd ..
	cd scheme && $(MAKE) clean && cd ..

update:
	cvs -q update -dP

