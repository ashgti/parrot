PERL     = ${perl}
RM_F     = ${rm_f}
PARROT   = ../../${test_prog}$(EXE)

all :
	$(PARROT) -o parrot.pbc parrot.pasm
	$(PARROT) parrot.pbc < sample.pasm > sample.pbc
# This can be enabled, when dumping Eval PMCs to string works
# $(PARROT) parrot.pbc < sample.pasm > sample.pbc
# $(PARROT) sample.pbc

clean :
	$(RM_F) core
	$(RM_F) *.pbc

