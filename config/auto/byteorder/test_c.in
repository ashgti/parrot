#include <stdlib.h>
#include <stdio.h>

int main() {
	int i;
	union W {
		unsigned char b[sizeof(${iv})/sizeof(unsigned char)];
		${iv} w;
	} w;
	if(sizeof(w) == 4) {
		w.w = 0x04030201;
	}
	else {
                w.w = 0x08070605; 
                w.w <<= 32; 
                w.w |= 0x04030201;
	}

	for(i = 0; i < sizeof(w.b); i++) {
		printf("%1u", w.b[i]);
	}
	printf("\n");
	exit(0);
	return 0;
}
