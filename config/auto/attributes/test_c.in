/*
 * figure out if the compiler supports attributes.
 * see parrot/compiler.h for attribute value definitions.
 */

#include <stdio.h>
#include <stdlib.h>
#include "parrot/compiler.h"


__attribute__malloc__
__attribute__noreturn__
__attribute__warn_unused_result__
__attribute__deprecated__
int
dummyfunc(int x)
    __attribute__format__(1,2,3)
    __attribute__nonnull__(1)
    ;


__attribute__const__
int
constfunc(int x);


__attribute__pure__
int
purefunc(int x);


int
useless(void) { int x __attribute__unused__; }


/* as long as the file compiles, everything is okay */
int
main(int argc, char *argv[])
{
    return EXIT_SUCCESS;
}


