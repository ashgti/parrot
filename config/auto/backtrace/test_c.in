figure out if libc has backtrace() & backtrace_symbols()
*/

#include <execinfo.h>
#include <stdlib.h>

int
main(int argc, char **argv)
{
    void *array[10];
    size_t size;
    char **strings;
    size = backtrace(array, 10);
    strings = backtrace_symbols(array, size);
    free(strings);
    return EXIT_SUCCESS;
}

