/*
** dan.ops
*/

#include "parrot/dynext.h"
VERSION = PARROT_VERSION;

=head1 NAME

dan.ops - Parrot ops emitted by Dan's compiler

=head1 DESCRIPTION

A dynamic oplib loadable at demand.

=over 4

=item B<mul>(out INT, in INT, in NUM)

=cut

inline op mul(out INT, in INT, in NUM) :base_core {
  FLOATVAL lhs = $2 * $3;
  $1 = (INTVAL)lhs;
  goto NEXT();
}

=back
