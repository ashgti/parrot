TEMPLATE Parrot_null_x {
    if (MAP[1]) {
        jit_emit_mov_ri<_N>(NATIVECODE, MAP[1], <val>);
    }
    else {
        jit_emit_mov_MI<_N>(NATIVECODE, ROFFS_INT(1), <val>);
    }
}

