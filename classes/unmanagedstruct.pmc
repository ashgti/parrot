#include "parrot/parrot.h"
#include "parrot/method_util.h"

pmclass UnManagedStruct {

    STRING* name () {
        return whoami;
    }

    INTVAL is_equal (PMC* value) {
        return (SELF->vtable == value->vtable
		&& SELF->data == value->data);
    }

    INTVAL defined () {
        return SELF->data != NULL;
    }

}
