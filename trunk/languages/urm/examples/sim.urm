### Tests if r1 and r2 are identical

in(r1,r2)
out(r3)

1: r3 <- 0
2: r3 <- r3 + 1
3: if r1 = 0 goto 7
4: if r2 = 0 goto 8
5: r1 <- r1 - 1
6: r2 <- r2 - 1
7: goto 2
8: if r2 = 0 goto 11
9: r3 <- 0
10: if r1 = 0 goto 11
11: r3 <- 0
